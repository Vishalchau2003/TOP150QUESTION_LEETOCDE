class Solution {
public:
    bool isSubsequence(string s, string t) {
        int n=s.size();
        int m=t.size();
        int i=0;
        int j=0;
        int cnt=0;
        while(i<n&&j<m){
            if(s[i]==t[j]){
                i++;
                j++;
                cnt++;
            }
            else{
                j++;
            }
        }  
        return cnt==n;
    }
};
